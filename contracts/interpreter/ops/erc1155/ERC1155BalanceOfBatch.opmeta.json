{
    "name": "ERC1155_BALANCE_OF_BATCH",
    "desc": "Get the balances of an ERC1155 token for batches of accounts and token ids and insert it into the stack",
    "operand": [
        {
            "name": "Inputs",
            "bits": [0, 7],
            "validRange": [[3, 255]],
            "computation": "(arg - 1) / 2"
        }
    ],
    "outputs": {
        "bits": [0, 7]
    },
    "inputs": {
        "bits": [0, 7],
        "computation": "(bits * 2) + 1",
        "parameters": [
            {
                "name": "Token Address"
            },
            {
                "name": "Account",
                "validLength": [[1, 127]],
                "length": "(inputs - 1) / 4"
            },
            {
                "name": "Token Id",
                "validLength": [[1, 127]],
                "length": "(inputs - 1) / 4"
            }
        ]
    },
    "aliases": [
        "ERC1155BALANCEOFBATCH",
        "IERC1155BALANCEOFBATCH"
    ]
}
